{
  "name": "Zodiacs Auto Run",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,

  "background": {
    "service_worker": "background.js"
  },

  "icons": { "128": "icon_128.png" },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "storage", "scripting", "tabs"]
}
